account contact@fabiancodes.de
host mail.fabiancodes.de
port 465
from contact@fabiancodes.de
user contact@fabiancodes.de
passwordeval "cat ~/random/contact@fabiancodes.de"

auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/fabian/.cache/msmtp/msmtp.log
tls_starttls off


