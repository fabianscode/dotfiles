account contact@fabianspecht.xyz
host mail.fabianspecht.xyz
port 465
from contact@fabianspecht.xyz
user contact@fabianspecht.xyz
passwordeval "pass show pers/contact@fabianspecht.xyz"

auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/fabian/.cache/msmtp/msmtp.log
tls_starttls off


